{
	"env": {
		"es2020": true,
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 2020
	},
	"extends": "eslint:recommended",
	"rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ]
	}
}
